<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_spazieren">

    <fragment
        android:id="@+id/navigation_spazieren"
        android:name="com.example.project2.ui.spazieren.SpazierenFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_spazieren" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.project2.ui.dashboard.DashboardFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_settings"
        android:name="com.example.project2.ui.settings.SettingsFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_notifications" />
    <fragment
        android:id="@+id/navigation_laufen"
        android:name="com.example.project2.ui.laufen.LaufenFragment"
        android:label="LaufenFragment"
        tools:layout="@layout/fragment_laufen" />
</navigation>